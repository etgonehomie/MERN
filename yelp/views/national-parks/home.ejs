<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>National Parks</title>
  </head>

  <body>
    <ul>
      <% parks.forEach(park => { %>
      <li>
        <a href="/national-parks/<%= park._id %>"
          ><%= park.title %> @ <%= park.location %></a
        >
      </li>
      <% }) %>
    </ul>
  </body>
</html>
